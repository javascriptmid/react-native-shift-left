{
  "transformIgnorePatterns": [
    "node_modules/(?!react-native|react-navigation|@react-navigation|@react-native-community)"
  ],
  "testPathIgnorePatterns": ["/node_modules/", "/e2e/"],
  "preset": "react-native",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80
    }
  },
  "setupFiles": ["./jest/NativeModules.js"]
}
